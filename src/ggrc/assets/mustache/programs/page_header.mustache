<h1 class="{{model.category}}">
  <div class="grcobject {{model.category}}" rel="tooltip" data-placement="bottom" title="{{model.title_singular}}">
    <i class="grcicon-{{model.table_singular}}"></i>
  </div>
  <span>
    <strong>Program:</strong>
  </span>
  {{instance.title}}
  {{#if instance.private}}
  <span class="private">
    <i class="grcicon-private" rel="tooltip" title="Private"></i>
  </span>
  {{/if}}
</h1>
